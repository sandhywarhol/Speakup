{
  "version": 2,
  "builds": [
    {
      "src": "ui/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/desktop.html"
    },
    {
      "src": "/desktop.html",
      "dest": "/desktop.html"
    },
    {
      "src": "/petitions",
      "dest": "/petitions-desktop.html"
    },
    {
      "src": "/create-petition",
      "dest": "/create-petition-desktop.html"
    },
    {
      "src": "/profile",
      "dest": "/profile-desktop.html"
    },
    {
      "src": "/settings",
      "dest": "/settings-desktop.html"
    },
    {
      "src": "/timeline",
      "dest": "/timeline-desktop.html"
    },
    {
      "src": "/admin/petitions",
      "dest": "/admin-petitions-desktop.html"
    },
    {
      "src": "/admin/verified-badge",
      "dest": "/admin-verified-badge-desktop.html"
    },
    {
      "src": "/request-verified-badge",
      "dest": "/request-verified-badge-desktop.html"
    },
    {
      "src": "/terms-privacy",
      "dest": "/terms-privacy-desktop.html"
    },
    {
      "src": "/oauth-callback",
      "dest": "/oauth-callback.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}